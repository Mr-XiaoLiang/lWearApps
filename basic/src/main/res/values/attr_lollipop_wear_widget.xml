<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="activeColor" format="color" />
    <attr name="inactiveColor" format="color" />

    <declare-styleable name="CircularProgressIndicator">
        <attr name="activeColor" />
        <attr name="inactiveColor" />
        <attr name="indicatorProgress" format="float" />
        <attr name="startAngle" format="float" />
        <attr name="sweepAngle" format="float" />
        <attr name="interval" format="dimension" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="antiClockwise" format="boolean" />
    </declare-styleable>
    <declare-styleable name="HorizontalPageIndicator">
        <attr name="activeColor" />
        <attr name="inactiveColor" />
        <attr name="indicatorInterval" format="dimension" />
        <attr name="indicatorRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ArcLayout_Layout">
        <attr name="layout_arc_angle" format="float" />
        <attr name="layout_arc_radius" format="dimension" />
        <attr name="layout_arc_margin" format="dimension" />
        <attr name="layout_arc_mode" format="enum">
            <enum name="minEdge" value="0" />
            <enum name="maxEdge" value="1" />
            <enum name="height" value="2" />
            <enum name="width" value="3" />
        </attr>
    </declare-styleable>
</resources>